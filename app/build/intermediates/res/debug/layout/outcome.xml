<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/outcome_linear_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#000000" >

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Handling on Departure"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#ffffff"
                    android:textSize="32dp"
                    android:textStyle="bold"
                    android:typeface="normal" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <CheckBox
                    android:id="@+id/outcome_walking_unaided"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Walking un-aided" />

                <CheckBox
                    android:id="@+id/outcome_walking_aided"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Walking Aided" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <CheckBox
                    android:id="@+id/outcome_stretcher"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Stretcher" />

                <CheckBox
                    android:id="@+id/outcome_longboard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Long Board" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <CheckBox
                    android:id="@+id/outcome_carry_chair"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Carry chair" />

                <CheckBox
                    android:id="@+id/outcome_orthopaedic_stretcher"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Orthopaedic Stretcher" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Other"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <EditText
                    android:id="@+id/outcome_departure_other"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ems="10" />
            </TableRow>
        </TableLayout>

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TableRow
                android:id="@+id/tableRow5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#000000" >

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Transport"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#ffffff"
                    android:textSize="32dp"
                    android:textStyle="bold"
                    android:typeface="normal" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <CheckBox
                    android:id="@+id/outcome_own_transport"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Own Transport" />

                <CheckBox
                    android:id="@+id/outcome_public_transport"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Public Transport" />

                <CheckBox
                    android:id="@+id/outcome_nhs_ambulance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NHS Ambulance" />
            </TableRow>
        </TableLayout>

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TableRow
                android:id="@+id/tableRow9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#000000" >

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Outcome"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#ffffff"
                    android:textSize="32dp"
                    android:textStyle="bold"
                    android:typeface="normal" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <CheckBox
                    android:id="@+id/outcome_treatment_completed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Treatment Completed" />

                <CheckBox
                    android:id="@+id/outcome_advised_to_seek"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Advised to Seek further medical treatment" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <CheckBox
                    android:id="@+id/outcome_hospital"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Hospital" />

                <CheckBox
                    android:id="@+id/outcome_review_later"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Review Later" />

            </TableRow>

            <TableRow
                android:id="@+id/tableRow12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Time left first aid"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <TimePicker
                    android:id="@+id/outcome_time_left_first_aid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </TableRow>

        </TableLayout>

    </LinearLayout>

</ScrollView>
