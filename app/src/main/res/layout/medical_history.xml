<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbarStyle="insideOverlay"
        android:scrollbars="vertical" >


     <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="2dp"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbarStyle="insideOverlay"
        android:scrollbars="vertical" >

         <TableRow
             android:id="@+id/tableRow10"
             android:layout_width="wrap_content"
             android:background="#000000"
             android:layout_height="wrap_content" >

             <TextView
                 android:id="@+id/textView10"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="History of Presenting Complaint"
                 android:textAppearance="?android:attr/textAppearanceMedium"
                 android:textColor="#ffffff"
                 android:textSize="32dp"
                 android:textStyle="bold"
                 android:typeface="normal" />

         </TableRow>

         <TableRow
             android:id="@+id/tableRow12"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content" >

             <EditText
                 android:id="@+id/medical_history_presenting_complaint"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:ems="10"
                 android:inputType="textMultiLine"
                 android:lines="3"
                 android:minLines="3" >
             </EditText>

         </TableRow>

          <TableRow
            android:id="@+id/tableRow0"
            android:layout_width="wrap_content"
            android:background="#000000"
            android:layout_height="wrap_content" >

              <TextView
                  android:id="@+id/textView1"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="Medical Conditions"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:textColor="#ffffff"
                  android:textSize="32dp"
                  android:textStyle="bold"
                  android:typeface="normal" />
      
        </TableRow>


        
        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

        </TableRow>
        
        
         <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

             <LinearLayout
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:orientation="vertical" >

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content" >

                     <CheckBox
                         android:id="@+id/medical_history_high_blood_pressure"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="High Blood Pressure" />

                     <CheckBox
                         android:id="@+id/medical_history_stroke"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Stroke" />

                     <CheckBox
                         android:id="@+id/medical_history_seizures"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Seizures" />

                     <CheckBox
                         android:id="@+id/medical_history_diabetes"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Diabetes" />
                 </LinearLayout>

                 <LinearLayout
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content" >

                     <CheckBox
                         android:id="@+id/medical_history_cardiac"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Cardiac" />

                     <CheckBox
                         android:id="@+id/medical_history_asthma"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Asthma" />

                     <CheckBox
                         android:id="@+id/medical_history_respiratory"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Respiratory" />


                 </LinearLayout>

             </LinearLayout>

        </TableRow>
         
        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Other?"
                    android:textAppearance="?android:attr/textAppearanceMedium" />

                <EditText
                    android:id="@+id/medical_history_other"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:inputType="textMultiLine"
                    android:lines="3" />

            </LinearLayout>

        </TableRow>

         <TableRow
             android:id="@+id/tableRow15"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content" >

             <RelativeLayout
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content" >

                 <TextView
                     android:id="@+id/textView14"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:text="FAST Test"
                     android:textSize="18dp"
                     android:textStyle="bold"
                     android:textAppearance="?android:attr/textAppearanceMedium" />

                 <RadioGroup
                     android:id="@+id/medical_history_fast"
                     android:layout_width="wrap_content"
                     android:layout_height="wrap_content"
                     android:layout_below="@id/textView14"
                     android:orientation="horizontal" >

                     <RadioButton
                         android:id="@+id/medical_history_fast_yes"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:checked="false"
                         android:text="Positive" />

                     <RadioButton
                         android:id="@+id/medical_history_fast_no"
                         android:layout_width="wrap_content"
                         android:layout_height="wrap_content"
                         android:text="Negative" />
                 </RadioGroup>
             </RelativeLayout>
         </TableRow>
        
          <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="wrap_content"
            android:background="#000000"
            android:layout_height="wrap_content" >

              <TextView
                  android:id="@+id/textView7"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="Known allergies"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:textColor="#ffffff"
                  android:textSize="32dp"
                  android:textStyle="bold"
                  android:typeface="normal" />
      
        </TableRow>

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <EditText
                android:id="@+id/medical_history_allergies"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textMultiLine"
                android:lines="3"
                android:minLines="3" >
            </EditText>

        </TableRow>


          <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:background="#000000"
            android:layout_height="wrap_content" >

              <TextView
                  android:id="@+id/textView11"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="List Medication(s)"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:textColor="#ffffff"
                  android:textSize="32dp"
                  android:textStyle="bold"
                  android:typeface="normal" />
      
        </TableRow>

        <TableRow
            android:id="@+id/tableRow5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <EditText
                android:id="@+id/medical_history_medication"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:inputType="textMultiLine"
                android:lines="3"
                android:minHeight="3dp"
                android:minLines="3" />

        </TableRow>
     
   </TableLayout>
    

</ScrollView>
